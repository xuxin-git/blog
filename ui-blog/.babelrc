{
    "presets": [
        "@babel/react",
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": "30",
                    "ie": "9"
                }
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "babel-plugin-syntax-dynamic-import",
        "transform-class-properties",
        ["import", { "libraryName": "antd", "style": true }]
    ]
}
